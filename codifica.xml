   <text xml:lang="fr" xml:id="pagina18" subtype="manuscript">
      <body>
         <div type="pagina" n="18">
            <!-- Page beginnng codifica la pagina-->
            <pb n="18" facs="#P18Intera" />
            <!--codifica la pericope-->
            <ab type="pericope" n="1" xml:id="Pericope1_p18">
               <lb xml:id="riga1_peri1_pag18" n="1" facs="#riga1p18" />
               <!-- Line beginning -->
               En quoi consiste le <term ref="#changementmorphologique"> changement
               <choice> <abbr>morpol</abbr> <expan>morp<ex>h</ex>ol<ex>ogique</ex></expan></choice> </term> qui
               <lb xml:id="riga2_peri1_pag18" n="2" facs="#riga2p18" />
               s'accomplit d'<choice><sic>1</sic><corr resp="#EF">une</corr></choice>
               <term ref="#epoque"><choice><abbr>époq<am>.</am></abbr> <expan>époq<ex>ue</ex></expan> </choice> </term> 
               à l'autre
               <choice><sic>:</sic><corr resp="#EF">?</corr></choice>
            </ab>
            <ab type="pericope" n="2" xml:id="Pericope2_p18">
               <lb xml:id="riga3_peri2_pag18" n="3" facs="#riga3p18" />
               1°dans l'<term ref="#analyse">analyse</term> <choice><abbr>différ<am>.</am></abbr><expan>différ<ex>ente</ex></expan></choice>
               des mêmes
               <lb xml:id="riga4_peri2_pag18" n="4" facs="#riga4p18" />
               <term ref="#formes">formes</term>, ou la <term ref="#valeur">valeur</term> 
               <choice><abbr>différ<am>.</am></abbr><expan>différ<ex>ente</ex></expan></choice>
               que la <term ref="#langue">langue</term> 
               <lb xml:id="riga5_peri2_pag18" n="5" facs="#riga5p18" />
               leur attribue, ou le <term ref="#rapport">rapport</term> 
               <choice><abbr>différ<am>.</am></abbr><expan>différ<ex>ente</ex></expan></choice>
               qu'elle
               <lb xml:id="riga6_peri2_pag18" n="6" facs="#riga6p18" />
               établit entre
               <choice> <sic>elle</sic> <corr resp="#EF">elles</corr></choice> <choice> <sic>;</sic> <corr resp="#EF">:</corr> </choice>
               tous <term ref="#faits">faits</term>  qui restent
               <lb xml:id="riga7_peri2_pag18" n="7" facs="#riga7p18" />
               dans le <term ref="#domaine">domaine</term>  purement
               <choice> <sic> <gap reason="illegible" atLeast="4" atMost="6" unit="chars" />logique</sic> 
               <corr resp="#EF">psychologique</corr></choice>,
               <lb xml:id="riga8_peri2_pag18" n="8" facs="#riga8p18" />
               mais n'en sont pas moins
               <choice> <sic>d</sic> <corr resp="#EF">des</corr> </choice>
               <term ref="#faits">faits</term> positifs.
            </ab>
            <ab type="pericope" n="3" xml:id="Pericope3_p18">
               <lb xml:id="riga9_peri3_pag18" n="9" facs="#riga9p18" />
               <choice> 
                    <abbr> Ep <am>.</am> </abbr>
                  <expan>E<ex>xem</ex>p<ex>le</ex></expan> </choice>
               <choice>
                  <sic>.</sic> <corr resp="#EF">:</corr>
               </choice>
               <choice>
                  <abbr> Epoq <am>.</am> </abbr>
                  <expan xml:lang="it">Epoca</expan>
               </choice>
               I
               <choice>
                  <sic>.</sic>
                  <corr resp="#EF" />
               </choice>
               <mentioned xml:lang="el">βέλεσ-σι</mentioned>
               <lb xml:id="riga10_peri3_pag18" n="10" facs="#riga10p18" />
               <choice>
                  <abbr> Ep <am>.</am> </abbr>
                  <expan xml:lang="it"> Ep<ex>oca</ex>
                  </expan>
               </choice>
               II
               <choice>
                  <sic>.</sic>
                  <corr resp="#EF" />
               </choice>
               <mentioned xml:lang="el">βέλ-εσσι</mentioned>
               <lb xml:id="riga11_peri3_pag18" n="11" facs="#riga11p18" />
               2°
               <choice>
                  <abbr>Ds</abbr>
                  <expan> d<ex>an</ex>s </expan>
               </choice>
               la création de <term ref="#formes">formes</term>  nouvelles,
               <lb xml:id="riga12_peri3_pag18" n="12" facs="#riga12p18" />
               <term ref="#fait">fait</term> plus tangible, plus matériel :
               <lb xml:id="riga13_peri3_pag18" n="13" facs="#riga13p18" />
               <choice>
                  <abbr> Ep <am>.</am> </abbr>
                  <expan xml:lang="it"> Ep<ex>oca</ex></expan>
               </choice>
               I
               <choice>
                  <sic>.</sic>
                  <corr resp="#EF" />
               </choice>
               <mentioned xml:lang="el">θηρσί</mentioned>
               <lb xml:id="riga14_peri3_pag18" n="14" facs="#riga14p18" />
               <choice>
                  <abbr> Ep <am>.</am> </abbr>
                  <expan xml:lang="it"> Ep<ex>oca</ex></expan>
               </choice>
               II
               <choice>
                  <sic>.</sic>
                  <corr resp="#EF" />
               </choice>
               <mentioned xml:lang="el">θήρσσι</mentioned>
               (<choice>
                  <abbr> créat <am>.</am> </abbr>
                  <expan> créat<ex>ion</ex></expan>
               </choice>
               <choice>
                  <abbr> nouv<am>.</am> </abbr>
                  <expan> nouv<ex>elle</ex></expan>
               </choice>)
               <choice>
                  <sic />
                  <corr resp="#EF">.</corr>
               </choice>
            </ab>
            <ab type="pericope" n="4" xml:id="Pericope4_p18">
               <lb xml:id="riga15_peri4_pag18" n="15" facs="#riga15p18" />
               Reprenons le
               <choice>
                  <sic>1er</sic>
                  <corr resp="#EF">premier</corr>
               </choice>
                  <term ref="#fait">fait</term> :
               <choice>
                  <sic>Le</sic>
                  <corr resp="#EF">le</corr>
               </choice>
               <term ref="#changement">changement</term> 
               <lb xml:id="riga16_peri4_pag18" n="16" facs="#riga16p18" />
               survenu
               <choice>
                  <sic>d</sic>
                  <corr resp="#EF">dans</corr>
               </choice>
               l'<term>aperception</term>  de <mentioned xml:lang="el">βέλεσσι</mentioned> par
               <lb xml:id="riga17_peri4_pag18" n="17" facs="#riga17p18" />
               <choice>
                  <sic>La</sic>
                  <corr resp="#EF">la</corr>
               </choice>
               <term ref="#langue">
               <choice>  
                  <abbr>lgue</abbr>
                  <expan>l<ex>an</ex>gue</expan> 
               </choice> 
               </term>
               <choice>
                  <sic>,</sic>
                  <corr resp="#EF" />
               </choice>
               resterait lettre close si nous en
               <lb xml:id="riga18_peri4_pag18" n="18" facs="#riga18p18" />
               cherchions la <term ref="#raison">raison</term> 
               <choice>
                  <sic>d</sic>
                  <corr resp="#EF">dans</corr>
               </choice>
               cette <term ref="#forme">forme</term>  elle- <choice> <abbr>mê</abbr> <expan>mê<ex>me</ex></expan></choice>.
               <lb xml:id="riga19_peri4_pag18" n="19" facs="#riga19p18" />
               Elle a sa <term ref="#source">source</term> 
               <choice>
                  <abbr> uniq <am>.</am></abbr>
                  <expan> uniq<ex>ue</ex></expan>
               </choice>
               <emph rend="underline">
                  <choice>
                     <sic>Dans</sic>
                     <corr resp="#EF">dans</corr>
                  </choice>
                  les <term ref="#formes">formes</term> 
                  <lb xml:id="riga20_peri4_pag18" n="20" facs="#riga20p18" />
                  concurrentes
               </emph>
               , ainsi que
               <choice>
                  <abbr>ns</abbr>
                  <expan> n<ex>ou</ex>s</expan>
               </choice>
               l'avons
               <add place="above">déjà dit</add>
               <del rend="overstrike">
                  <gap reason="illegible" quantity="2" unit="character" />
               </del>
               .
            </ab>
            <ab type="pericope" n="5" xml:id="Pericope5_p18">
               <lb xml:id="riga21_peri5_pag18" n="21" facs="#riga21p18" />
               Comme l'
              <term ref="#élément"> <choice> <!--chiedere Vincenzo-->
                  <abbr> élém<am>.</am> </abbr>
                  <expan>élém<ex>ent</ex></expan>
               </choice> </term>- <mentioned xml:lang="el" rend="underline">εσ</mentioned> - ne se retrouve
               <lb xml:id="riga22_peri5_pag18" n="22" facs="#riga22p18" />
               pas dans
               <mentioned xml:lang="el">βέλει</mentioned> ,
               <mentioned xml:lang="el">βελέων</mentioned>
               <choice>
                  <sic />
                  <corr resp="#EF">,</corr>
               </choice> etc
               <choice>
                  <sic>...</sic>
                  <corr resp="#EF">.</corr>
               </choice>
               <choice>
                  <sic />
                  <corr resp="#EF">,</corr>
               </choice> depuis
               <lb xml:id="riga23_peri5_pag18" n="23" facs="#riga23p18" />
               <choice>
                  <sic>La</sic>
                  <corr resp="#EF">la</corr>
               </choice>
               <term ref="#chute">chute</term> de l'
               <emph rend="underline">s</emph> , la <term ref="#langue">langue</term> 
               <del rend="overstrike"> ne voit
                  <lb xml:id="riga24_peri5_pag18" n="24" facs="#riga24p18" />
                  pt pas et ce
                  <gap reason="illegible" quantity="4" unit="character" />
               </del>
               n'a aucune indi
               <lb xml:id="riga25_peri5_pag18" n="25" facs="#riga25p18" />
               cation qui lui permette de couper
               <lb xml:id="riga26_peri5_pag18" n="26" facs="#riga26p18" />
               <mentioned xml:lang="el">βέλεσ-σι</mentioned>, et elle coupe
               <choice>
                  <abbr>maint</abbr> <expan> maint<ex>enant</ex> </expan>
               </choice>
               <mentioned xml:lang="el">βέλ-εσσι</mentioned>.
               <lb xml:id="riga27_peri5_pag18" n="27" facs="#riga27p18" />
               Ainsi le <term ref="#mouvement">mouvement</term> ne s'est pas produit
               <lb xml:id="riga28_peri5_pag18" n="28" facs="#riga28p18" />
               entre <mentioned xml:lang="el">βέλεσ-σι</mentioned>
               <lb xml:id="riga29_peri5_pag18" n="29" facs="#riga29p18" />
               <choice>
                  <sic>N</sic>
                  <corr resp="#EF">et</corr>
               </choice>
               <mentioned xml:lang="el">βέλ-εσσι</mentioned>, ce qui serait
